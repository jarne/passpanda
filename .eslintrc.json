{
    "root": true,
    "extends": ["react-app", "plugin:jsdoc/recommended"],
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "jsdoc/require-jsdoc": "off"
    },
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
